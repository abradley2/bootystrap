nav(class='navbar navbar-inverse')
  div(class='container-fluid')
    div(class='navbar-header')
      button(
        type='button' 
        class='navbar-toggle collapsed' 
        data-toggle='collapse' 
        data-target='#navigation-list'
        aria-expanded='false'
        )
        span(class='sr-only') Toggle navigation
      // ko if: hasHome
      a(class='navbar-brand' href='#home') Home
      // /ko 
    div(class='collapse navbar-collapse' id='navigation-list')
      ul(class='nav navbar-nav' data-bind='foreach: navlinks')
        //ko if: _.isUndefined($data.get('subCategories'))
        li
          a(data-bind='component: {name: "humble-navlink", params: $data}')      
        // /ko
        //ko ifnot: _.isUndefined($data.get('subCategories'))
        li
          ul(data-bind='component: {name: "humble-nav-dropdown", params: $data}')
        // /ko
        
